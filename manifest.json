{
    "manifest_version": 2,
    "name": "Hide Short Rides",
    "version": "0.1",
  
    "description": "Hide from the Strava feed short (<10km) bicycle rides. Nobody cares about them at all",
    "permissions": [
        "https://www.strava.com/dashboard"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "run_at": "document_end",
            "js": ["removeRides.js"]
        }
    ]

}
